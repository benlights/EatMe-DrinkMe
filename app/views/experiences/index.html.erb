<h1>All Experiences</h1>
<% if @experiences %>
  <% @experiences.each do |experience| %>
    <%= link_to "#{experience.venue.name}", venue_path(experience.venue) %>
    <% if experience.dishes %>
      <ul>
        <%= render partial: experience.dishes %>
      </ul>
    <% end %>
  <% end %>
<% end %>

<div>
  <%= link_to 'New Experience', new_experience_path %>
</div>